# Overwrite text editor settings - https://EditorConfig.org

# # top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
indent_style = space
trim_trailing_whitespace = true
charset = utf-8

[*.{py,sql,json}]
indent_style = space
indent_size = 4
insert_final_newline = true

[Makefile]
indent_style = tab
